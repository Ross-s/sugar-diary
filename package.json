{
  "name": "sugar-diary",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "phase run next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "biome:check": "biome check .",
    "biome:lint": "biome lint .",
    "biome:format": "biome format --write .",
    "biome:fix": "biome check --write .",
    "db:generate": "phase run drizzle-kit generate",
    "db:migrate": "phase run drizzle-kit migrate",
    "preview": "phase run next build --turbopack && phase run next start"
  },
  "dependencies": {
    "@libsql/client": "^0.15.10",
    "@stackframe/stack": "2.8.25",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "daisyui": "^5.0.47",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.3",
    "next": "15.4.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
